      <data key="d5">5723130cfdc3cfe1f19ce6306b4a689d</data>
    </edge>
    <edge source="&quot;BANK&quot;" target="&quot;COUNTY RECORDS&quot;">
      <data key="d3">1.0</data>
      <data key="d4">"Banks use county records to prepare ownership and encumbrance reports for real estate transactions."</data>
      <data key="d5">5723130cfdc3cfe1f19ce6306b4a689d</data>
    </edge>
    <edge source="&quot;BANK&quot;" target="&quot;12 CFR 22&quot;">
