      <data key="d5">273cb1ace9888fefd2938eba7529519a</data>
    </edge>
    <edge source="&quot;OCC&quot;" target="&quot;AG PORTFOLIO&quot;">
      <data key="d3">1.0</data>
      <data key="d4">"The OCC is responsible for examining the Ag portfolio to assess risk, growth, and changes."</data>
      <data key="d5">273cb1ace9888fefd2938eba7529519a</data>
    </edge>
    <edge source="&quot;OCC&quot;" target="&quot;ROE&quot;">
      <data key="d3">1.0</data>
